{
    "manifest_version": 2,

    "name": "domain changer",
    "version": "1.0",

    "permissions": [
        "tabs",
        "storage"
    ],

    "icons": {
        "16" : "16.png",
        "32" : "32.png",
        "48" : "48.png",
        "128" : "128.png"
    },

    "browser_action": {
        "default_title": "change domain",
        "default_icon": "48.png",
        "default_popup": "popup.html"
    }
}
